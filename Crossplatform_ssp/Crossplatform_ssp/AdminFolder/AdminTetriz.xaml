<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Crossplatform_ssp.AdminFolder.AdminTetriz"
             Title="Tetriz">

    <StackLayout VerticalOptions="StartAndExpand" HorizontalOptions="StartAndExpand" Orientation="Vertical">
        <Button Text="Opret Tetriz Begivenhed" x:Name="opretTBBtn" VerticalOptions="Center" HorizontalOptions="StartAndExpand" />
        <Button Text="Opret Tetriz Opslag" x:Name="opretTOBtn" VerticalOptions="Center" HorizontalOptions="StartAndExpand" />
        <Button Text="Opdater Tetriz Begivenheder" Clicked="tetBeg" VerticalOptions="Center" HorizontalOptions="StartAndExpand"/>
        <Button Text="Opdater Tetriz Opslag" Clicked="tetOpslag" VerticalOptions="Center" HorizontalOptions="StartAndExpand"/>
    </StackLayout>
</ContentPage>